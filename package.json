{
	"name": "pokemon-lounge",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"build:raspi": "ng build --base-href \"/pokemon-lounge/\" --output-path=/var/www/pokemon-lounge",
		"build:gh-pages": "ng build --base-href \"/pokemon-lounge/\"",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"deploy:gh-pages": "npm run build:gh-pages && npm run copy:gh-pages",
		"copy:gh-pages": "rimraf docs && cpy dist/pokemon-lounge/browser/** docs --parents && echo > docs/.nojekyll",
		"lint": "ng lint"
	},
	"prettier": {
		"overrides": [
			{
				"files": "*.html",
				"options": {
					"parser": "angular"
				}
			}
		]
	},
	"private": true,
	"dependencies": {
		"@angular/common": "^20.1.0",
		"@angular/compiler": "^20.1.0",
		"@angular/core": "^20.1.0",
		"@angular/forms": "^20.1.0",
		"@angular/platform-browser": "^20.1.0",
		"@angular/router": "^20.1.0",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.15.0"
	},
	"devDependencies": {
		"@angular-eslint/builder": "^20.1.1",
		"@angular-eslint/eslint-plugin": "^20.1.1",
		"@angular-eslint/eslint-plugin-template": "^20.1.1",
		"@angular/build": "^20.1.0",
		"@angular/cli": "^20.1.0",
		"@angular/compiler-cli": "^20.1.0",
		"@types/jasmine": "~5.1.0",
		"angular-eslint": "20.1.1",
		"cpy-cli": "^5.0.0",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.5.1",
		"jasmine-core": "~5.8.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"prettier": "^3.6.2",
		"rimraf": "^6.0.1",
		"typescript": "~5.8.2",
		"typescript-eslint": "8.34.1"
	}
}
